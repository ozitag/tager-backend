{
    "name": "ozitag/tager-backend",
    "type": "project",
    "description": "Tager Backend",
    "license": "MIT",
    "require": {
        "ozitag/tager-backend-core": "^1.3",
        "ozitag/tager-backend-sentry": "^1.3",
        "ozitag/tager-backend-admin": "^1.2",
        "ozitag/tager-backend-administrators": "^1.0",
        "ozitag/tager-backend-backup": "^0.0.1",
        "ozitag/tager-backend-cron": "^1.0",
        "ozitag/tager-backend-seo": "^1.4",
        "ozitag/tager-backend-mail": "^1.5",
        "ozitag/tager-backend-settings": "^1.6",
        "ozitag/tager-backend-menus": "^1.3",
        "ozitag/tager-backend-pages": "^1.2",
        "ozitag/tager-backend-rbac": "^1.0",
        "ozitag/tager-backend-crud": "^1.2",
        "ozitag/tager-backend-http-cache": "^1.0",
        "ozitag/tager-backend-utils": "^1.1",
        "ozitag/tager-backend-fields": "^1.0",
        "ozitag/tager-backend-panel": "^1.0",
        "ozitag/tager-backend-files": "^1.0",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
